    "test:all": "pnpm run test && pnpm run test:e2e && pnpm run validate:tokens",
    "clean": "node scripts/clean.js",
    "clean:all": "node scripts/clean.js && node -e \"require('fs').rmSync('node_modules', { recursive: true, force: true }); require('fs').rmSync('pnpm-lock.yaml', { force: true });\"",
